
        <!DOCTYPE web-app PUBLIC
                "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>com.wyy.server.UserServer.HelloWorld</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.wyy.server.UserServer.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>com.wyy.server.Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Queryuser</servlet-name>
        <servlet-class>com.wyy.server.UserServer.QueryUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Regist</servlet-name>
        <servlet-class>com.wyy.server.UserServer.Regist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deluser</servlet-name>
        <servlet-class>com.wyy.server.UserServer.Deluser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Createteam</servlet-name>
        <servlet-class>com.wyy.server.TeamServer.Createteam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Updatamsg</servlet-name>
        <servlet-class>com.wyy.server.UserServer.Updatamsg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Changelv</servlet-name>
        <servlet-class>com.wyy.server.UserServer.Changelv</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/A</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Queryuser</servlet-name>
        <url-pattern>/queryuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Regist</servlet-name>
        <url-pattern>/regist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deluser</servlet-name>
        <url-pattern>/deluser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Createteam</servlet-name>
        <url-pattern>/createteam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Updatamsg</servlet-name>
        <url-pattern>/updatamsg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Changelv</servlet-name>
        <url-pattern>/changelv</url-pattern>
    </servlet-mapping>

</web-app>
